package overriding;

public class Lion {
	public String gender;
	public int weight;
	public int size;
	public String name;
	
	public void howl() {
		System.out.println("으르렁");
	}
	public void getInfo() {
		System.out.println("성별 : " + gender + ", 무게 : " + weight +
				           ", 크기 : " + size + ", 이름 :" + name);
	}
}
