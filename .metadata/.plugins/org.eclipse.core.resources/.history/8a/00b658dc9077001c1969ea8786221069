package overloading;

	public class Calculator {
		
		   public void plus(int a) {// 파라미터로 int a 값만 요구
			   System.out.println(a + "에 1을 더한 값은 " + ( a + 1) + "입니다.");
		   }
		   public void plus(int a, int b) {// 파라미터로 int a, int b를 요구
				System.out.println(a + " 에 " + b + "를 더한 값은 " + (a + b) + "입니다.");
       }
		   public void plus() { // 파라미터값으로 double a 요구
			   
		   }
		  
		   public void plus() { // ㅏ라미터값으로 String str 요구
			   
		   }
}
